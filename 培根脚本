    <script>
        // æ‚¬æµ®çƒåŠŸèƒ½
        const floatingBall = document.getElementById('floatingBall');
        const floatingMenu = document.getElementById('floatingMenu');
        
        floatingBall.addEventListener('click', function() {
            floatingMenu.classList.toggle('active');
            floatingBall.innerHTML = floatingMenu.classList.contains('active') ? 
                '<i class="fas fa-times"></i>' : '<i class="fas fa-plus"></i>';
        });
        
        // ç‚¹å‡»é¡µé¢å…¶ä»–åŒºåŸŸå…³é—­æ‚¬æµ®èœå•
        document.addEventListener('click', function(e) {
            if (!floatingBall.contains(e.target) && !floatingMenu.contains(e.target)) {
                floatingMenu.classList.remove('active');
                floatingBall.innerHTML = '<i class="fas fa-plus"></i>';
            }
        });
        
        // è„šæœ¬åŠŸèƒ½
        function copyScript(scriptId) {
